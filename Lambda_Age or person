people = [
    {"name": "Asha", "age": 25},
    {"name": "John", "age": 16},
    {"name": "Kumar", "age": 30},
    {"name": "Rita", "age": 14}
]

adults = list(filter(lambda p: p["age"] >= 18, people))
adults_names = list(map(lambda p: p["name"], adults))
print(adults_names)


