fib_n = lambda n: n if n <= 1 else fib_n(n-1) + fib_n(n-2)


n_terms = 10
series_lambda = [fib_n(i) for i in range(n_terms)]
print(f"Fibonacci series using recursive lambda: {series_lambda}")


